{
  "manifest_version": 3,
  "name": "Medusa Curse Extension",
  "version": "1.0",
  "description": "A chaotic mythological curse wrapped in a browser extension.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/contentScript.js"]
    }
  ]
}
